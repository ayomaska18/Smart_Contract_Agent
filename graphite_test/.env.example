# OpenAI Configuration (Required for client service)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Ethereum/Blockchain Configuration (Required for server service)
METAMASK_PRIVATE_KEY=your_metamask_private_key_here
ETHEREUM_SEPOLIA_RPC=https://sepolia.infura.io/v3/your_project_id
WALLET_ADDRESS=0xYourWalletAddressHere

